{
  "name": "pangolin",
  "version-string": "0.5",
  "port-version": 14,
  "description": "Lightweight GUI Library",
  "homepage": "https://github.com/stevenlovegrove/Pangolin",
  "supports": "!uwp & !osx",
  "dependencies": [
    "eigen3",
    {
      "name": "ffmpeg",
      "default-features": false,
      "features": [
        "avformat"
      ]
    },
    {
      "name": "glew",
      "platform": "!android & !ios"
    },
    "libjpeg-turbo",
    "libpng"
  ]
}
