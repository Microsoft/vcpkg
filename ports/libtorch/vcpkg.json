{
  "name": "libtorch",
  "version-string": "v1.8.1",
  "description": "Tensors and Dynamic neural networks in Python with strong GPU acceleration",
  "homepage": "https://pytorch.org/",
  "supports": "x64 & (windows | osx | linux)",
  "dependencies": [
    "cpuinfo",
    "eigen3",
    "fbgemm",
    "fmt",
    "fp16",
    "gflags",
    "glog",
    "lmdb",
    "protobuf",
    {
      "name": "tensorpipe",
      "platform": "linux | osx"
    },
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    "zstd"
  ],
  "default-features": [
    "xnnpack"
  ],
  "features": {
    "cuda": {
      "description": "Build with NVIDIA CUDA GPU backend",
      "dependencies": [
        "cuda",
        "cudnn"
      ]
    },
    "leveldb": {
      "description": "Build with LevelDB",
      "dependencies": [
        "leveldb",
        "snappy"
      ]
    },
    "nnpack": {
      "description": "Build with NNPack",
      "dependencies": [
        "nnpack"
      ]
    },
    "opencl": {
      "description": "Build with OpenCL",
      "dependencies": [
        "opencl"
      ]
    },
    "opencv3": {
      "description": "Build with OpenCV 3.x",
      "dependencies": [
        "opencv3"
      ]
    },
    "qnnpack": {
      "description": "Build with QNNPack",
      "dependencies": [
        "qnnpack"
      ]
    },
    "tbb": {
      "description": "Build with Intel TBB",
      "dependencies": [
        "tbb"
      ]
    },
    "vulkan": {
      "description": "Build with Vulkan GPU backend",
      "dependencies": [
        "vulkan"
      ]
    },
    "xnnpack": {
      "description": "Build with XNNPack",
      "dependencies": [
        "xnnpack"
      ]
    }
  }
}
